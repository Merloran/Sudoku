<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.comp.Controller">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="%FileMenu">
               <items>
                  <MenuItem fx:id="saveButton" mnemonicParsing="false" onAction="#saveBoard" text="%SaveFile" />
                  <MenuItem mnemonicParsing="false" onAction="#loadBoard" text="%LoadFile" />
               </items>
          </Menu>
        </menus>
      </MenuBar>
      <ButtonBar buttonMinWidth="52.5" prefHeight="34.0" prefWidth="573.0">
        <buttons>
            <Button onAction="#fillField" mnemonicParsing="false" text="1" />
            <Button onAction="#fillField" mnemonicParsing="false" text="2" />
            <Button onAction="#fillField" mnemonicParsing="false" text="3" />
            <Button onAction="#fillField" mnemonicParsing="false" text="4" />
            <Button onAction="#fillField" mnemonicParsing="false" text="5" />
            <Button onAction="#fillField" mnemonicParsing="false" text="6" />
            <Button onAction="#fillField" mnemonicParsing="false" text="7" />
            <Button onAction="#fillField" mnemonicParsing="false" text="8" />
            <Button onAction="#fillField" mnemonicParsing="false" text="9" />
        </buttons>
         <padding>
            <Insets right="20.0" />
         </padding>
      </ButtonBar>
      <GridPane fx:id="pane">
         <padding>
            <Insets bottom="10.0" left="150.0" right="150.0" top="80.0" />
         </padding>
      </GridPane>
      <Pane prefHeight="82.0" prefWidth="600.0">
         <children>
            <Button layoutX="260.0" layoutY="10.0" mnemonicParsing="false" onAction="#backMenu" text="%BackButton" />
         </children>
      </Pane>
   </children>
</VBox>
